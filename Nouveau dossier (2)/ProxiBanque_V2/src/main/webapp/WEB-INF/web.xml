<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  

  <servlet>
  	<servlet-name>ServletAccueil</servlet-name>
  	<display-name>ServletAccueil</display-name>
  	<servlet-class>fr.gtm.presentation.ServletAccueil</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletEditionClient</servlet-name>
  	<display-name>ServletEditionClient</display-name>
  	<servlet-class>fr.gtm.presentation.ServletEditionClient</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletListClient</servlet-name>
  	<display-name>ServletListClient</display-name>
  	<servlet-class>fr.gtm.presentation.ServletListClient</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletListCompte</servlet-name>
  	<display-name>ServletListCompte</display-name>
  	<servlet-class>fr.gtm.presentation.ServletListCompte</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletVirement</servlet-name>
  	<display-name>ServletVirement</display-name>
  	<servlet-class>fr.gtm.presentation.ServletVirement</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>ServletAccueil</servlet-name>
  	<url-pattern>/ServletAccueil</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletEditionClient</servlet-name>
  	<url-pattern>/ServletEditionClient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletListClient</servlet-name>
  	<url-pattern>/ServletListClient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletListCompte</servlet-name>
  	<url-pattern>/ServletListCompte</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletVirement</servlet-name>
  	<url-pattern>/ServletVirement</url-pattern>
  </servlet-mapping>
  
  
  <!-- 1. Page d'accueil -->
	<welcome-file-list>
		<welcome-file>accueil.jsp</welcome-file>
	</welcome-file-list>
</web-app>
